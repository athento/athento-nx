<?xml version="1.0"?>
<component name="org.athento.nuxeo.athento-nx-security.restAPI-contrib">

    <extension target="org.nuxeo.ecm.platform.ui.web.restAPI.service.PluggableRestletService"
               point="restlets">

        <restletPlugin
                name="athDownload"
                class="org.athento.nuxeo.security.restlet.DownloadRestlet"
                enabled="true"
                useSeam="true">
            <urlPatterns>
                <urlPattern>/athdownload/{repo}/{docid}/{xpath}</urlPattern>
            </urlPatterns>
        </restletPlugin>

    </extension>

</component>